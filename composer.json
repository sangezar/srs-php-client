{
    "name": "sangezar/srs-php-client",
    "description": "PHP client for SRS (Simple Realtime Server) API",
    "type": "library",
    "version": "1.0.0",
    "keywords": ["srs", "streaming", "api-client", "rtmp", "srt", "rtmp-server", "rtmp-client", "rtmp-streaming"],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "SrsClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SrsClient\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "cs": "phpcs",
        "cs-fix": "phpcbf"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sangezar",
            "email": "andrey.karnaukh94@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
